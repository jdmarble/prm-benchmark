<launch>
  <arg name="sampler" default="UniformValidStateSampler"/>
  <param name="sampler" value="$(arg sampler)"/>
  <rosparam>
    environment: cubicles
    start: [0.0,   70.0, 40.0, 1.0, 0.0, 0.0, 0.0]
    goal:  [150.0, 70.0, 50.0, 1.0, 0.0, 0.0, 0.0]
    StateValidityCheckingResolution: 0.01
    runtime_limit: 240.0
    memory_limit: 10000.0
    run_count: 31
  </rosparam>
  <node name="$(anon ompl-benchmark)" pkg="ompl-benchmark" type="ompl-benchmark"
    required="true" cwd="node" output="screen"/>
</launch>
